{
  "name": "@api-platform/create-client",
  "version": "0.10.0",
  "description": "Generate apps built with Next, Nuxt, Quasar, React, React Native, Vue or Vuetify for any API documented using Hydra or OpenAPI",
  "files": [
    "dist",
    "src",
    "templates"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "package.json": "./package.json"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/api-platform/create-client.git"
  },
  "keywords": [
    "api",
    "api-platform",
    "client",
    "generator",
    "hydra",
    "openapi",
    "react",
    "vue",
    "next",
    "nuxt",
    "quasar",
    "vuetify",
    "react-native",
    "typescript",
    "create-client"
  ],
  "homepage": "https://github.com/api-platform/create-client",
  "bugs": {
    "url": "https://github.com/api-platform/create-client/issues"
  },
  "author": "KÃ©vin Dunglas",
  "license": "MIT",
  "devDependencies": {
    "@commander-js/extra-typings": "^12.0.1",
    "@playwright/test": "1.43.1",
    "@types/esutils": "^2.0.2",
    "@types/node": "^20.12.7",
    "@types/sprintf-js": "^1.1.4",
    "@types/tmp": "^0.2.6",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.13",
    "picocolors": "^1.0.0",
    "tmp": "^0.2.3",
    "tsup": "^8.0.2",
    "type-fest": "^4.15.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.6.0",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "@api-platform/api-doc-parser": "^0.16.4",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "esutils": "^2.0.3",
    "handlebars": "^4.7.8",
    "handlebars-helpers": "^0.10.0",
    "playwright-ssr": "^0.0.3",
    "prettier": "^3.2.5",
    "sprintf-js": "^1.1.3"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "test": "vitest",
    "lint": "eslint src",
    "fix": "eslint --fix src",
    "build": "tsup-node",
    "test-gen": "./testgen.sh",
    "test-gen-openapi3": "rm -rf ./tmp && pnpm build && ENTRYPOINT=https://demo.api-platform.com/docs.jsonopenapi FORMAT=openapi3 ./testgen.sh",
    "test-gen-custom": "rm -rf ./tmp && pnpm build && babel src/generators/ReactGenerator.js src/generators/BaseGenerator.js -d ./tmp/gens && cp -r ./templates/react ./templates/react-common ./templates/entrypoint.js ./tmp/gens && ./lib/index.js https://demo.api-platform.com ./tmp/react-custom -g \"$(pwd)/tmp/gens/ReactGenerator.js\" -t ./tmp/gens",
    "test-gen-env": "rm -rf ./tmp && pnpm build && API_PLATFORM_CREATE_CLIENT_ENTRYPOINT=https://demo.api-platform.com API_PLATFORM_CREATE_CLIENT_OUTPUT=./tmp ./lib/index.js",
    "test-app": "./testapp.sh",
    "test-react-app": "./testapp.sh react",
    "test-next-app": "./testapp.sh next",
    "test-vue-app": "./testapp.sh vue",
    "test-nuxt-app": "./testapp.sh nuxt"
  },
  "lint-staged": {
    "src/**/*.js": "pnpm fix"
  },
  "bin": {
    "create-client": "./dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 18.17.0",
    "pnpm": ">= 8.0.0"
  },
  "packageManager": "pnpm@8.15.5+sha256.4b4efa12490e5055d59b9b9fc9438b7d581a6b7af3b5675eb5c5f447cee1a589"
}